            <Input id="Matkl" value="{Matkl}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />

            <Label text="{i18n>StateConfirm}" labelFor="StateConfirm" />
            <CheckBox id="StateConfirm" enabled="{StateConfirm}" editable="false" />
