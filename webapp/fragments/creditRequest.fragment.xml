<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <VBox>
        <VBox displayInline="false" class="sapUiContentPadding">
            <Label text="{i18n>ItemDescription}" labelFor="Txz01" />
