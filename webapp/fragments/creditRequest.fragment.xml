            <Label text="{i18n>Price}" labelFor="Preis" />
            <Input id="Preis" value="{ path: 'Preis', formatter: '.formatter.currencyValue' }" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>Currency}" labelFor="Waers" />
            <Input id="Waers" value="{Waers}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>AccountAssignment}" labelFor="Knttp" />
            <Input id="Knttp" value="{Knttp}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>WBS}" labelFor="PsPspPnr" />
