            <Label text="{i18n>Asset}" labelFor="Anln1" />
            <Input id="Anln1" value="{Anln1}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>Network}" labelFor="Nplnr" />
            <Input id="Nplnr" value="{Nplnr}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>DeliveryDate}" labelFor="Lfdat" />
            <Input id="Lfdat" value="{Lfdat}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>Buyer}" labelFor="Afnam" />
            <Input id="Afnam" value="{Afnam}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />

            <Label text="{i18n>BuyerName}" labelFor="Buyer" />
            <Input id="Buyer" value="{Buyer}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>BuyerPhone}" labelFor="PhoneB" />
