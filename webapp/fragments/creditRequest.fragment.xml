            <Label text="{i18n>BuyerEmail}" labelFor="EmailB" />
            <Input id="EmailB" value="{EmailB}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />

            <Label text="{i18n>BuyingReason}" labelFor="Reason" />
            <Input id="Reason" value="{Reason}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />

            <Label text="{i18n>Vendor}" labelFor="Vendor" />
            <Input id="Vendor" value="{Vendor}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>VendorAddress}" labelFor="AddressV" />
            <Input id="AddressV" value="{AddressV}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />
            <Label text="{i18n>VendorEmail}" labelFor="EmailV" />
            <Input id="EmailV" value="{EmailV}" editable="false" showValueHelp="true" valueHelpRequest=".onVendorValueHelpRequest" />

            <Label text="{i18n>ArrivalMethod}" labelFor="Arrival" />
